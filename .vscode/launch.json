  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "saxParser",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/test/saxParser.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "saxStream",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/test/saxStream.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "getCells",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/test/getCells.js"
    },
    {
      "type": "node-terminal",
      "name": "---",
      "request": "launch"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "HtmlDataParser",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/test/HtmlDataParser.test.js"
    },
    {
      "type": "node",
      "name": "HtmlDataReader",
      "program": "${workspaceFolder}/test/HtmlDataReader.test.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "RowAsObjectTransform",
      "program": "${workspaceFolder}/test/RowAsObject.test.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "RepeatCellTransform",
      "program": "${workspaceFolder}/test/RepeatCell.test.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "RepeatHeadingTransform",
      "program": "${workspaceFolder}/test/RepeatHeading.test.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node-terminal",
      "name": "--- hdp",
      "request": "launch"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "hdp helloworld json",
      "program": "html-data-parser.js",
      "args": [ ".\\test\\data\\html\\helloworld.html", "--id=global", "--headers=Greating" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "hdp helloworld csv",
      "program": "html-data-parser.js",
      "args": [ ".\\test\\data\\html\\helloworld.html", "--id=cosmic", "--headers=Greating", "--format=csv" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "hdp helloworld rows",
      "program": "html-data-parser.js",
      "args": [ ".\\test\\data\\html\\helloworld.html", "--format=rows" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "hdp ANSI csv",
      "program": "html-data-parser.js",
      "args": [ ".\\test\\data\\html\\ansi.html", ".\\test\\output\\hdp\\ansi.csv", "--format=csv" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "hdp ANSI json",
      "program": "html-data-parser.js",
      "args": [ ".\\test\\data\\html\\ansi.html", ".\\test\\output\\hdp\\ansi.json", "--heading=Congressional Districts", "--format=json" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "hdp RepeatCell",
      "program": "html-data-parser.js",
      "args": [ "--options=.\\test\\RepeatCell.options.json" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "hdp RepeatHeading",
      "program": "html-data-parser.js",
      "args": [ "--options=.\\test\\RepeatHeading.options.json" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
